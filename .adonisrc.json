{
  "typescript": true,
  "commands": ["./src/cmd", "@adonisjs/core/build/commands/index.js"],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "./src/app",
    "Config": "./src/config",
    "Database": "./src/database",
    "Contracts": "./src/contracts"
  },
  "preloads": ["./src/start/routes", "./src/start/kernel"],
  "providers": ["./src/providers/AppProvider", "@adonisjs/core"],
  "metaFiles": [
    {
      "pattern": "src/public/**",
      "reloadServer": false
    }
  ],
  "aceProviders": [],
  "tests": {
    "suites": [
      {
        "name": "functional",
        "files": ["tests/functional/**/*.spec(.ts|.js)"],
        "timeout": 60000
      }
    ]
  },
  "testProviders": ["@japa/preset-adonis/TestsProvider"],
  "directories": {
    "config": "src/config",
    "public": "src/public",
    "contracts": "src/contracts",
    "providers": "src/providers",
    "database": "src/database",
    "migrations": "src/database/migrations",
    "seeds": "src/database/seeders",
    "resources": "src/resources",
    "views": "src/resources/views",
    "start": "src/start",
    "tmp": "src/tmp",
    "tests": "tests"
  }
}
